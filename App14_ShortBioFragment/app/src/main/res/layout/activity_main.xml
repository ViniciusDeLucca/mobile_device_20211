<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <include layout="@layout/fragment_logotipo"
        android:id="@+id/layLogo"
    />
    <com.google.android.material.tabs.TabLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/layLogo"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/tblMenu"
            android:background="#870101"/>

    <androidx.viewpager.widget.ViewPager
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tblMenu"
            app:layout_constraintStart_toStartOf="parent"
            android:background="#FF9191"
            android:id="@+id/vpgVisualizador"/>

    <!--include layout="@layout/fragment_sobre">

    </include> !-->



</androidx.constraintlayout.widget.ConstraintLayout>